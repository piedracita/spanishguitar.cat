{
  "sitemap.xml": {
    "Protocol": "https",
    "DomainName": "spanishguitar.cat",
    "Root": "./",
    "IncludeExt": [
      ".html",
      ".php"
    ],
      "Exclude": [
      "common-php/**",
      "php/**"
    ],    
    "TagsToInclude": [
      "priority",
      "lastmod"
    ],
    "DefaultChangeFrequency": "monthly",
    "TabCharacters": "\t",
    "bIncludeWWW": true,
    "bRemoveFileExtentions": false,
    "bUseTrailingSlash": false,
    "bAutomaticallyUpdateSitemap": true,
    "bMinimized": false
  }
}